{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
	  "module": "esnext",
	  "target": "esnext",
	  "lib": ["dom", "esnext"],
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "types": ["svelte"]
	},
	"moduleResolution": "node",
	"types": ["node", "svelte"],
	"include": ["src/**/*", "src/**/*.svelte", "src/types"],
	"exclude": ["node_modules"]
  }